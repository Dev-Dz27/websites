{
  "name": "website",
  "version": "0.1.0",
  "author": {
    "name": "Jerome Fitzgerald",
    "email": "j@jeromefitzgerald.com",
    "url": "https://jeromefitzgerald.com"
  },
  "license": "MIT",
  "scripts": {
    "analyze": "BUNDLE_ANALYZE=both yarn build",
    "build": "next build",
    "dev": "next dev",
    "debug": "NODE_OPTIONS='--inspect' next dev",
    "find:unused": "next-unused",
    "lint-typescript": "yarn typescript",
    "lint-eslint": "eslint . --ext js,jsx,ts,tsx --max-warnings=0",
    "lint": "yarn lint-typescript && yarn prettier-check && yarn lint-eslint",
    "lint-fix": "yarn prettier-fix && eslint . --ext js,jsx,ts,tsx --fix --max-warnings=0",
    "postbuild": "next-sitemap --config next.sitemap.js",
    "pretty-quick": "pretty-quick",
    "prettier-check": "prettier --check \"**/*.{js,jsx,ts,tsx,json,md,mdx,css,html,yml,yaml,scss}\"",
    "prettier-fix": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md,mdx,css,html,yml,yaml,scss}\"",
    "start": "next start",
    "typescript": "tsc --noEmit --declaration"
  },
  "dependencies": {
    "@reach/portal": "0.12.1",
    "@reach/skip-nav": "0.12.1",
    "@tailwindcss/typography": "0.3.1",
    "@tailwindcss/ui": "0.7.2",
    "@types/body-scroll-lock": "2.6.1",
    "@vercel/fetch": "6.1.0",
    "autoprefixer": "10.1.0",
    "body-scroll-lock": "3.1.5",
    "clsx": "1.1.1",
    "comma-number": "2.0.1",
    "date-fns": "2.16.1",
    "date-fns-tz": "1.0.12",
    "fathom-client": "3.0.0",
    "firebase": "8.2.1",
    "firebase-admin": "9.4.2",
    "github-slugger": "1.3.0",
    "htmlescape": "1.1.1",
    "intersection-observer": "0.12.0",
    "ioredis": "4.19.4",
    "js-cookie": "2.2.1",
    "keen-slider": "5.3.5",
    "lodash": "4.17.20",
    "ms": "2.1.3",
    "next": "10.0.5-canary.10",
    "next-seo": "4.17.0",
    "next-themes": "0.0.10",
    "nprogress": "0.2.0",
    "postcss": "8.2.2",
    "postcss-nesting": "7.0.1",
    "preact": "10.5.9",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "react-icons": "4.1.0",
    "react-intersection-observer": "8.31.0",
    "react-jsx-parser": "1.28.1",
    "swr": "0.3.11",
    "tailwind-underline-utils": "1.1.2",
    "tailwindcss": "2.0.2",
    "title": "3.4.2",
    "uuid": "8.3.2"
  },
  "devDependencies": {
    "@next/bundle-analyzer": "10.0.4",
    "@types/js-cookie": "2.2.6",
    "@types/ms": "0.7.31",
    "@types/nanoid": "2.1.0",
    "@types/node": "14.14.19",
    "@types/node-fetch": "2.5.7",
    "@types/nprogress": "0.2.0",
    "@types/reach__skip-nav": "0.1.1",
    "@types/react": "17.0.0",
    "@types/uuid": "8.3.0",
    "@typescript-eslint/eslint-plugin": "4.11.1",
    "@typescript-eslint/parser": "4.11.1",
    "concurrently": "5.3.0",
    "cross-env": "7.0.3",
    "eslint": "7.17.0",
    "eslint-config-prettier": "7.1.0",
    "eslint-plugin-react": "7.22.0",
    "eslint-plugin-react-hooks": "4.2.0",
    "fs-extra": "9.0.1",
    "globby": "11.0.1",
    "husky": "4.3.6",
    "lint-staged": "10.5.3",
    "next-compose-plugins": "2.2.1",
    "next-sitemap": "1.3.29",
    "next-unused": "0.0.3",
    "postcss-flexbugs-fixes": "5.0.2",
    "postcss-hover-media-feature": "1.0.0",
    "postcss-preset-env": "6.7.0",
    "pre-commit": "1.2.2",
    "prettier": "2.2.1",
    "pretty-quick": "3.1.0",
    "rimraf": "3.0.2",
    "typescript": "4.1.3"
  },
  "resolutions": {
    "webpack": "5.11.1"
  },
  "next-unused": {
    "alias": {
      "~components/*": [
        "components/*"
      ],
      "~config/*": [
        "config/*"
      ],
      "~context/*": [
        "context/*"
      ],
      "~lib/*": [
        "lib/*"
      ],
      "~styles/*": [
        "styles/*"
      ],
      "~utils/*": [
        "utils/*"
      ]
    },
    "debug": true,
    "include": [
      "components",
      "config",
      "context",
      "hooks",
      "lib",
      "pages",
      "styles",
      "utils"
    ],
    "exclude": [
      "config/notion/schema/defaults.ts",
      "config/notion/website/getRedirects.js",
      "components/Modal/*",
      "components/NowPlaying/NowPlaying.module.css",
      "components/NowPlaying/NowPlayingWithControls.tsx",
      "lib/firebase.ts",
      "lib/notion/init/createPageWithInlineTableItem.ts",
      "lib/spotify/mock/mockTrack.ts",
      "utils/isObjectEmpty.ts"
    ],
    "entrypoints": [
      "pages"
    ]
  }
}
