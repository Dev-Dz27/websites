{
  "version": 2,
  "regions": ["iad", "sfo"],
  "public": false,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/next@canary"
    }
  ],
  "cleanUrls": true,
  "trailingSlash": false,
  "headers": [
    {
      "source": "/static/fonts/inter/inter-var-latin.woff2",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ]
}
